.errors
    - unless @errors.nil?
        - @errors.each do |msg|
            = msg

= form_for @member, :url => 'signin' do |f|
    .group
        .label= f.label "Login:"
        .field= f.text_field :login

    .group
        .label= f.label "Has≈Ço:"
        .field= f.password_field :password

    .button= f.submit